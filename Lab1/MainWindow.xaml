<Window x:Class="Lab1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab1"
        mc:Ignorable="d"
        Title="MainWindow" Height="352" Width="448">
    <Grid>
        <RadioButton x:Name="rbBank" Content="Bank Charges" HorizontalAlignment="Center" Margin="0,98,0,0" VerticalAlignment="Top" FontSize="20" Width="262" Checked="rbBank_Checked"/>
        <RadioButton x:Name="rbShipping" Content="Fast Freight Shipping" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="20" Checked="rbShipping_Checked" Width="262" Margin="0,151,0,0"/>
        <RadioButton x:Name="rbPopulation" Content="Population of Organisms" HorizontalAlignment="Center" Margin="0,205,0,0" VerticalAlignment="Top" FontSize="20" Checked="rbPopulation_Checked" Width="262"/>
        <Label x:Name="lblLab1" Content="Desktop Development Lab 1" HorizontalAlignment="Center" Margin="0,32,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold"/>
        <Button x:Name="btnGo" Content="Go" HorizontalAlignment="Center" Margin="0,262,0,0" VerticalAlignment="Top" FontSize="20" Width="112" Click="btnGo_Click"/>
    </Grid>
</Window>
